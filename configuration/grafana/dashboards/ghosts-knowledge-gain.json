      "targets": [
        {
          "alias": "",
          "datasource": {
            "type": "postgres",
            "uid": "ghosts"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  n.npcprofile->>'name' AS teacher_name,\n  COUNT(DISTINCT l.npcid) AS students_taught,\n  COUNT(DISTINCT l.topic) AS unique_topics,\n  SUM(l.value) AS total_knowledge_value\nFROM\n  npc_learning l\n  INNER JOIN npcs n ON l.fromnpcid = n.id\nWHERE\n  l.createdutc >= $__timeFrom()::timestamptz\n  AND l.createdutc <= $__timeTo()::timestamptz\nGROUP BY\n  n.id, n.npcprofile->>'name'\nORDER BY\n  students_taught DESC\nLIMIT 50",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
