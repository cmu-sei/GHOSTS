                "isheadless": "false",
                "blockimages": "true",
                "blockstyles": "true",
                "blockflash": "true",
                "blockscripts": "true",
                "stickiness": 75,
                "stickiness-depth-min": 5,
                "stickiness-depth-max": 10000,
